{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "target": "es2015"
    },
    "exclude": [
        "node_modules",
        "typings"
    ]
}
